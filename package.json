  "type": "module",
  "scripts": {
    "generate:api": "sh src/scripts/gen-api-code.sh",
    "locale": "zx ./src/store/locale/scripts/locale.mjs",
    "sync": "yarn generate:api && yarn locale",
    "prepare": "husky install",
    "start": "vite --host",
    "mock": "REACT_APP_API_SERVER=mock vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
